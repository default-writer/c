  "version": "0.2.0",
  "configurations": [
    {
      "name": "python/c++",
      "type": "pythoncpp",
      "request": "launch",
      "pythonLaunchName": "python",
      "cppAttachName": "c++"
    },
    {
      "name": "c++",
      "type": "cppdbg",
      "request": "attach",
      "program": "${workspaceFolder}/cmake/main-tests-vm1",
      "processId": ""
    },
    {
      "name": "python (default)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_default.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "python (default, sanitize)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_default.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal",
      "env": {
        "LD_PRELOAD": "/usr/lib/x86_64-linux-gnu/libasan.so.8"
      }
    },
    {
      "preLaunchTask": "shell: cmake",
      "name": "python (file)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_file.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (gc)",
      "name": "python (file, gc)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_file.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (verbose)",
      "name": "python (file, verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_file.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (gc, verbose)",
      "name": "python (file, gc, verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_file.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake",
      "name": "python (memory)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_memory.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (gc)",
      "name": "python (memory, gc)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_memory.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (verbose)",
      "name": "python (memory, verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_memory.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (gc, verbose)",
      "name": "python (memory, gc, verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_memory.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake",
      "name": "python (list)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_list.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (gc)",
      "name": "python (list, gc)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_list.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (verbose)",
      "name": "python (list, verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_list.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "shell: cmake (gc, verbose)",
      "name": "python (list, gc, verbose)",
      "type": "debugpy",
      "request": "launch",
      "program": "${workspaceFolder}/examples/python/test_list.py",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "preLaunchTask": "cmake: build",
      "name": "gdb",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/cmake/main-tests-vm1",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}/cmake",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Set Disassembly Flavor to Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "preLaunchTask": "shell: coverage",
      "name": "coverage",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/build/coverage-v6/main-tests-vm1",
      "args": [],
      "stopAtEntry": false,
      "cwd": "${workspaceFolder}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    },
    {
      "type": "bashdb",
      "request": "launch",
      "name": "bash",
      "terminalKind": "integrated",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}/bin/utils/cmake.sh",
      "args": [
        "--target=main-tests-vm1"
      ]
    }
  ]
}
