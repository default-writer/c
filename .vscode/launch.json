    "version": "0.2.0",
    "configurations": [
        {
            "name": "(lldb) list",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/main",
            "initCommands":["settings set target.disable-aslr false"]
        },
        {
            "name": "(lldb) list-light",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/main-light",
            "initCommands":["settings set target.disable-aslr false"]
        },
        {
            "name": "(lldb) list-micro",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/build/main-micro",
            "initCommands":["settings set target.disable-aslr false"]
        },
        {
            "name": "(gdb) list",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/coverage/main",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "miDebuggerPath": "/usr/bin/gdb"
        },
        {
            "name": "(gdb) list-light",
