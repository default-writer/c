{
  "clangd.path": "${workspaceFolder}/.tools/clangd/bin/clangd",
  "cmake.debugConfig": {
    "MIMode": "gdb",
    "miDebuggerPath": "/usr/bin/gdb",
    "setupCommands": [
      {
        "description": "Enable pretty-printing for lldb",
        "text": "-enable-pretty-printing",
        "ignoreFailures": true
      }
    ],
    "console": "integratedTerminal",
    "cwd": "${workspaceRoot}/cmake",
    "environment": []
  }
}
