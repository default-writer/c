{
    "files.associations": {
        "optional": "c",
        "random": "c",
        "bitset": "c",
        "chrono": "c",
        "string_view": "c",
        "bit": "c",
        "*.tcc": "c",
        "condition_variable": "c",
        "algorithm": "c",
        "istream": "c",
        "limits": "c",
        "mutex": "c",
        "shared_mutex": "c",
        "streambuf": "c",
        "thread": "c",
        "list": "cpp",
        "initializer_list": "cpp",
        "memory_resource": "cpp",
        "regex": "cpp",
        "type_traits": "cpp",
        "*.inc": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "unordered_map": "cpp",
        "vector": "cpp",
        "exception": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory": "cpp",
        "numeric": "cpp",
        "string": "cpp",
        "system_error": "cpp",
        "tuple": "cpp",
        "utility": "cpp",
        "fstream": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "typeinfo": "cpp",
        "api.h": "c",
        "data.h": "c",
        "alloc.h": "c",
        "globals.h": "c"
    },
    "lldb.displayFormat": "auto",
    "lldb.showDisassembly": "auto",
    "lldb.dereferencePointers": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "terminal.integrated.defaultProfile.linux": "bash",
    "terminal.integrated.profiles.linux": {
        "bash": {
          "path": "/bin/bash"
        }
    },
    "cmake.configureOnOpen": true,
    "gcovViewer.buildDirectories": [
        "/workspaces/c/"
    ],
    "C_Cpp.dimInactiveRegions": true,
    "C_Cpp.errorSquiggles": "Disabled",
    "coverage-gutters.showLineCoverage": true,
    "coverage-gutters.showRulerCoverage": true,
    "coverage-gutters.coverageBaseDir": ".",
    "coverage-gutters.coverageFileNames": [
        "main.info",
        "main-light.info",
        "main-micro.info",
        "main-tiny.info"
    ],
    "files.watcherInclude": [
        "."
    ],
    "sonarlint.pathToCompileCommands": "${workspaceFolder}\\build\\compile_commands.json"
}