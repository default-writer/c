{
    "files.associations": {
        "optional": "c",
        "random": "c",
        "bitset": "c",
        "chrono": "c",
        "string_view": "c",
        "bit": "c",
        "*.tcc": "c",
        "condition_variable": "c",
        "algorithm": "c",
        "istream": "c",
        "limits": "c",
        "mutex": "c",
        "shared_mutex": "c",
        "streambuf": "c",
        "thread": "c",
        "list": "cpp",
        "initializer_list": "cpp",
        "memory_resource": "cpp",
        "regex": "cpp",
        "type_traits": "cpp",
        "*.inc": "cpp"
    },
    "lldb.displayFormat": "auto",
    "lldb.showDisassembly": "auto",
    "lldb.dereferencePointers": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "googletest.failed",
                "settings": {
                    "foreground": "#f00"
                }
            },
            {
                "scope": "googletest.passed",
                "settings": {
                    "foreground": "#0f0"
                }
            },
            {
                "scope": "googletest.run",
                "settings": {
                    "foreground": "#0f0"
                }
            }
        ]
    },
    "gtest-adapter.debugConfig": "(gdb) Launch",
    "gcovViewer.buildDirectories": [
        "/workspaces/c"
    ]
}